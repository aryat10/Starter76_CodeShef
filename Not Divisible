#include <bits/stdc++.h>
using namespace std;

void arr(int n){
    for(int i=0;i<n;i++){
        if(i%2==0){
            cout << 1 << " " ;
        }
        else{
            cout << 0 << " " ;
        }
    }
    cout << endl;
}


int main() {
	int t;
	 cin >> t;
	 while(t--){
	     int n;
	     cin >> n;
	     arr(n);
	 }
	return 0;
}
